{
  "name": "blitzapp",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "authjs-capacitor-oauth-plugin"
  ],
  "type": "module",
  "main": "main.js",
  "scripts": {
    "updateauthplugin": "cd authjs-capacitor-oauth-plugin && yarn run build && cd .. && yarn remove authjs-capacitor-oauth-plugin && yarn add file:./authjs-capacitor-oauth-plugin && yarn android && adb -s ZY22BNTBL6 reverse tcp:3000 tcp:3000 && yarn dev --experimental-https && adb shell am force-stop com.milomode.app && adb shell monkey -p com.milomode.app -c android.intent.category.LAUNCHER 1",
    "build": "next build",
    "check": "next lint && tsc --noEmit",
    "db:generate": "prisma migrate dev",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "selfSigned": "next dev --experimental-https --hostname=insertYourIpHere",
    "android": "npx cap run android",
    "db:studio": "prisma studio",
    "dev": "next dev --turbo",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "db-seed": "NODE_ENV=development prisma db seed"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.7.2",
    "@capacitor/android": "^7.3.0",
    "@capacitor/browser": "^7.0.2",
    "@capacitor/core": "^7.3.0",
    "@capacitor/ios": "^7.3.0",
    "@capacitor/local-notifications": "^7.0.3",
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "@mui/material-nextjs": "^7.3.3",
    "@prisma/client": "^5.14.0",
    "@t3-oss/env-nextjs": "^0.10.1",
    "@tanstack/react-query": "^5.50.0",
    "@trpc/client": "^11.0.0-rc.446",
    "@trpc/react-query": "^11.0.0-rc.446",
    "@trpc/server": "^11.0.0-rc.446",
    "authjs-capacitor-oauth-plugin": "file:./authjs-capacitor-oauth-plugin",
    "geist": "^1.3.0",
    "google-auth-library": "^10.5.0",
    "next": "^15.0.1",
    "next-auth": "5.0.0-beta.25",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-is": "18.3.1",
    "server-only": "^0.0.1",
    "superjson": "^2.2.1",
    "uuid": "^13.0.0",
    "zod": "^3.23.3"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.3.0",
    "@types/eslint": "^8.56.10",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.1.0",
    "@capacitor/docgen": "^0.3.0",
    "rollup": "^4.30.1",
    "electron": "^36.4.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.0.1",
    "postcss": "^8.4.39",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "prisma": "^5.14.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.5.3"
  },
  "ct3aMetadata": {
    "initVersion": "7.38.1"
  },
  "resolutions": {
    "react-is": "^18.3.1",
    "string-width": "4.2.3",
    "wrap-ansi": "6.2.0",
    "strip-ansi": "6.0.1"
  },
  "packageManager": "yarn@1.22.17"
}
